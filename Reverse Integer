class Solution {
public:
    int reverse(int x) {
        int rev=0;
        
        while(x!=0)
        {
            int remainder=x%10;
            x=x/10;
            rev = rev * 10 + remainder;
            
        }
        return rev;

    }
};


Example 1:

Input: x = 123
Output: 321
Example 2:

Input: x = -123
Output: -321
Example 3:

Input: x = 120
Output: 21
